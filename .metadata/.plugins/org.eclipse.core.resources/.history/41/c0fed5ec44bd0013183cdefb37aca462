package com.ifelse;

public class IfHWElse {
public static void main(String [] args){
	int a =100;
	if (a>=90){
	System.out.println("A");
}
else if (a<90 &&  a>=80){
	System.out.println("B");
}
else if ( a<79 && a>=70){
	System.out.println("C");
}
else if (a <69 && a>=60){
	System.out.println("D");
}
else {
	System.out.println("F");
}
}
}
